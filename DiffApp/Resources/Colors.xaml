<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="TextPrimary" Color="#24292e"/>
    <SolidColorBrush x:Key="LineNumberForeground" Color="#aaaaaa"/>

    <SolidColorBrush x:Key="DiffBackgroundAdded" Color="#e6ffed" />
    <SolidColorBrush x:Key="DiffBackgroundRemoved" Color="#ffeef0" />
    <SolidColorBrush x:Key="DiffBackgroundModified" Color="#fff8c5" />
    <SolidColorBrush x:Key="DiffBackgroundImaginary" Color="#eff0f2" />
    <SolidColorBrush x:Key="DiffBackgroundUnchanged" Color="Transparent" />

    <SolidColorBrush x:Key="DiffHighlightAdded" Color="#b4e2c2" />
    <SolidColorBrush x:Key="DiffHighlightRemoved" Color="#fdb8c0" />
    <SolidColorBrush x:Key="DiffHighlightUnchanged" Color="Transparent" />

</ResourceDictionary>
